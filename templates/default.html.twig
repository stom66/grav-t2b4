        {% include 'partials/jumbotron.html.twig' %}
    {% endif %}
{% endblock %}

{% block content %}
    <div class="container">
        <div class="row">
            <div class="col">
                {{ page.content|raw }}
            </div>
        </div>
    </div>
{% endblock %}